<div id="<%= dom_id category %>">
  <tr>
    <td>
        <input type="checkbox" name="selectors" class="checkbox" value="<%= category.id %>"/>
    </td>
    <td><%= category.id %></td>
    <td><%= category.name %></td>
    <td><%= category.description %></td>
    <td>
      <button type="button" class="btn btn-info"><%= link_to "Edit", edit_category_path(category) %></button>
    </td>
    <td>
      <%= button_to "Delete", category, method: :delete, class: "btn btn-danger" %>
    </td>
  </tr>
</div>
