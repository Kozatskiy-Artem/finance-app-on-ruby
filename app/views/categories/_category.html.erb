<div id="<%= dom_id category %>">
  <tr>
    <td><%= index + 1 %></td>
    <td><%= category.name %></td>
    <td><%= category.description %></td>
    <td>
      <div class="settings">
        <%= link_to "", edit_category_path(category), class: "btn btn-info bi bi-pencil-square settings-buttons" %>
        <%= button_to "", category, method: :delete, class: "btn btn-danger bi bi-trash3 settings-buttons" %>
      </div>
    </td>
  </tr>
</div>
